		"vector": "cpp",
		"__hash_table": "cpp",
		"__tree": "cpp",
		"bitset": "cpp",
		"deque": "cpp",
		"map": "cpp",
		"queue": "cpp",
		"set": "cpp",
		"stack": "cpp",
		"unordered_map": "cpp"
	}
}